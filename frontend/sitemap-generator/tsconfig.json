{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "outDir": "../dist"
  },
  "include": [
    "generate-sitemap.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}